@model List<NmyBlog.Model.Category>

<blockquote class="layui-elem-quote">标签管理</blockquote>
<div style="padding: 10px">
    <div>
        <a class="layui-btn" href="/Adnn1n/Category/Add">
            <i class="layui-icon">&#xe608;</i> 添加
        </a>
    </div>


    <table class="layui-table">
        <thead>
            <tr>
                <th>ID</th>
                <th>创建时间</th>
                <th>标签名</th>
                <th>编号</th>
                <th>父编号</th>
                <th>备注</th>
                <th>管理</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var ca in Model)
            {

                <tr>
                    <td>@ca.Id</td>
                    <td>@ca.CreateDate.ToString("yyyy-MM-dd HH:mm")</td>
                    <td>@ca.CaName</td>
                    <td>@ca.Bh</td>
                    <td>@ca.Pbh</td>
                    <td>@ca.Remark</td>
                    <td>
                        <a href="/Adnn1n/Category/Add/@ca.Id"><i class="layui-icon">&#xe642;</i> 编辑</a>
                        <a href="/Adnn1n/Category/Del/@ca.Id" onclick="return confirm('是否确认删除？')"><i class="layui-icon">&#x1006;</i> 删除</a>
                    </td>
                </tr>
            }

        </tbody>
    </table>
</div>
